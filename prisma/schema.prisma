generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "sqlserver"
  url      = env("DATABASE_URL")
}

model Session {
  id        String   @id @default(uuid()) @db.NVarChar(36)
  sid       String   @unique @db.NVarChar(255)
  data      String   @db.NVarChar(Max)
  expires   DateTime @db.DateTime
  createdAt DateTime @default(now(), map: "DF__Session__created__07C12930") @db.DateTime
  updatedAt DateTime @default(now(), map: "DF__Session__updated__08B54D69") @updatedAt @db.DateTime

  @@index([expires], map: "idx_session_expires")
  @@index([sid], map: "idx_session_sid")
}

